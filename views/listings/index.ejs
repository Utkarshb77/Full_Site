<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>WanderLust</title>
</head>
<body>
    <h2>All Listings</h2>
    <ul>
        <% alllistings.forEach(function(listing) { %>
            <li>
                <a href="/listings/<%= listing._id %>"><%= listing.title %></a> - <%= listing.location %>
            </li>
        <% }); %>
    </ul>
    <form action="/listings/new" method="get"><button>Create New Listing</button>
    </form>
</body>
</html>